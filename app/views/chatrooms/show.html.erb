<h1>#<%= @chatroom.name %></h1>
<div id="messsage"data-chatroom-id="<%= @chatroom.id %>">
  <% @chatroom.message.each do |message| %>
    <div class="message-container" id="message-<%= message.id %>">
      <i class="author">
        <span><%= message.user.nickname %></span>
        <small><% message.created_at.strftime("%a %b %e at %l:%M%p") %></small>
     </i>
     <p><%= message.content  %></p>
    </div>
  <% end %>
</div>